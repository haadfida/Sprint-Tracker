$('#filter').html("<%= escape_javascript render(:partial => 'reports/sprint_filter',  :locals=>{ sprints: @sprints, form_action: :sprint } ) %>");
<% if @sprint.present? %>
  $('#header').html("<%= escape_javascript render(:partial => 'reports/sprint_report_header',  :locals=>{ sprint_id: @sprint&.id, action: :sprint_csv } ) %>");
<% end %>
$('#sprint_report_table').html("<%= escape_javascript render(:partial => 'reports/sprint_report_table',  :locals=>{ sprint_report_data: @sprint_report_data } ) %>");