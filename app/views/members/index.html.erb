 <div class="container">   
<h1>Members#index</h1>
<p>Find me in app/views/members/index.html.erb</p>
<div>   
        <%= link_to 'Add User', new_member_path %>   
</div>   
<br>   
    <table class="table table-striped table-bordered table-hover" id="users-datatable" data-source="<%= members_path(format: :json) %>">   
      <thead class="table-dark">   
      <tr>    
        <th>First Name</th> 
        <th>Last Name</th>   
        <th>Email</th>   
        <th>Role</th>   
      </tr>   
      </thead>   
      <tbody>
       <% @users.each do |user| %> 
        <tr>  
        <td><%= user.first_name %></td>  
        <td><%= user.last_name %></td>    
        <td><%= user.email %></td> 
        <td><%= user.role_id %></td> 
         

        </tr> 
       <% end %>   
      </tbody>
  </table>
</div>
      