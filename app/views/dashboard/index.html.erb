<div class="container">
  <p class="navbar-text float-right">
    <% if user_signed_in? %> Logged in as <strong><%= current_user.email %> </strong>.
      <%= link_to t('users.log_out'), destroy_user_session_path, method: :delete, :class => 'navbar-link'  %> |
      <%= link_to t('users.view company members'), users_url, :class => 'navbar-link'  %>|
      <%= link_to t('projects.project_header'), projects_url, :class => 'navbar-link'  %>
    <% else %>
      <%= link_to t('users.sign_up'), new_user_registration_path, :class => 'navbar-link'  %> |
      <%= link_to t('users.log_in'), new_user_session_path, :class => 'navbar-link'  %>
    <% end %>
  </p>
</div>