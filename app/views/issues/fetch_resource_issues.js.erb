<% if params[:assignee_id].present? %>
$('#nav-assignee').html("<%= escape_javascript render(:partial => 'issues/table',  :locals=>{ issues: @issues} ) %>");
<% elsif params[:creator_id].present? %>
$('#nav-creator').html("<%= escape_javascript render(:partial => 'issues/table',  :locals=>{ issues: @issues} ) %>");
<% elsif params[:reviewer_id].present? %>
$('#nav-reviewer').html("<%= escape_javascript render(:partial => 'issues/table',  :locals=>{ issues: @issues} ) %>");
<% else %>
$('#nav-assignee').html("<%= escape_javascript render(:partial => 'issues/table',  :locals=>{ issues: @issues} ) %>");
<% end %>

